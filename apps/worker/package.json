{
  "name": "@metapulse/worker",
  "version": "1.0.0",
  "description": "MetaPulse Worker Service - Background data processing and analysis",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/scripts/seed.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run db:generate"
  },
  "keywords": [
    "metapulse",
    "worker",
    "crypto",
    "defi",
    "background-jobs",
    "data-processing",
    "ai-analysis"
  ],
  "author": "MetaPulse Team",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@metapulse/db": "workspace:*",
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.2",
    "bull": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "groq-sdk": "^0.3.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "sentiment": "^5.0.2",
    "technicalindicators": "^3.1.0",
    "twitter-api-v2": "^1.15.2",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metapulse/metapulsev2.git",
    "directory": "apps/worker"
  },
  "bugs": {
    "url": "https://github.com/metapulse/metapulsev2/issues"
  },
  "homepage": "https://github.com/metapulse/metapulsev2#readme"
}
